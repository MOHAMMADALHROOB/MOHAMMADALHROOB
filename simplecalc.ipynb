def simple_calc(x,y,oper) :

  if oper == "+" :
    print(x+y)
  elif oper == "-" :
    print(x-y)
  elif oper == "*" :
    print(x*y)
  elif oper == "/" :
    print(x/y)
  elif oper == "**" :
    print(x**y)
  elif oper =="//":
    print(x//y)
  elif oper =="%":
    print(x%y)
  else :
    print("check your operator")
    
simple_calc(1,2,"**")
